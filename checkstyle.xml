<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- Suppress VP abbreviation warnings for Visual Paradigm plugin classes -->
    <suppress checks="AbbreviationAsWordInName"
              files="VPUseCasePlugin\.java"
              message="Abbreviation in name 'VPUseCasePlugin'"/>

    <suppress checks="AbbreviationAsWordInName"
              files="VPUseCasePluginTest\.java"
              message="Abbreviation in name 'VPUseCasePluginTest'"/>
</suppressions>
